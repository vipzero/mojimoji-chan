{
	"name": "v-yomi-chan",
	"version": "1.0.0",
	"description": "5ch読み上げデスクトップアプリ",
	"main": "build/main.js",
	"scripts": {
		"build": "webpack",
		"lint": "eslint src/**/*{ts,tsx} --fix",
		"start": "webpack -w",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"repository": "https://github.com/vipzero/v-yomi-chan",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo",
		"typescript"
	],
	"author": "vip0",
	"license": "MIT",
	"dependencies": {
		"@elzup/eslint-config": "^1.3.0",
		"@material-ui/core": "^4.4.3",
		"chch": "^1.1.0",
		"dayjs": "^1.8.16",
		"lodash": "^4.17.15",
		"react": "^16.8.6",
		"react-dom": "^16.9.0",
		"react-use": "^12.2.2"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.139",
		"@types/react": "^16.9.3",
		"@types/react-dom": "^16.9.1",
		"@types/webpack": "^4.39.2",
		"bufferutil": "^4.0.1",
		"electron": "^6.0.10",
		"electron-builder": "^21.2.0",
		"electron-connect": "^0.6.3",
		"electron-rebuild": "^1.8.6",
		"electron-reload-webpack-plugin": "^2.0.4",
		"eslint": "^6.4.0",
		"eslint-config-google": "^0.14.0",
		"spawn-sync": "^2.0.0",
		"ts-loader": "^6.1.2",
		"typescript": "3.6.3",
		"utf-8-validate": "^5.0.2",
		"webpack": "^4.41.0",
		"webpack-cli": "^3.3.9"
	},
	"build": {
		"productName": "mojimoji-chan",
		"appId": "dev.vipper.mojimojichan",
		"asar": true,
		"npmRebuild": false,
		"files": [
			"index.html",
			"build/**/*",
			"node_modules/**/*"
		],
		"publish": [
			{
				"provider": "github",
				"releaseType": "release"
			}
		]
	}
